  $as_echo_n "(cached) " >&6
else
  if test -n "$GEOS_CONFIG"; then
  ac_cv_prog_GEOS_CONFIG="$GEOS_CONFIG" # Let the user override the test.
else
  ac_prog_rejected=no
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in "$ac_geos_config_dir"
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    if test "$as_dir/$ac_word$ac_exec_ext" = "
   "; then
       ac_prog_rejected=yes
       continue
     fi
    ac_cv_prog_GEOS_CONFIG="$with_geos"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

if test $ac_prog_rejected = yes; then
  # We found a bogon in the path, so make sure we never use it.
  set dummy $ac_cv_prog_GEOS_CONFIG
  shift
  if test $# != 0; then
    # We chose a different compiler from the bogus one.
    # However, it has the same basename, so the bogon will be chosen
    # first if we set GEOS_CONFIG to just the basename; use the full file name.
    shift
    ac_cv_prog_GEOS_CONFIG="$as_dir/$ac_word${1+' '}$@"
  fi
fi
  test -z "$ac_cv_prog_GEOS_CONFIG" && ac_cv_prog_GEOS_CONFIG="no"
fi
fi
GEOS_CONFIG=$ac_cv_prog_GEOS_CONFIG
if test -n "$GEOS_CONFIG"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $GEOS_CONFIG" >&5
$as_echo "$GEOS_CONFIG" >&6; }
else
