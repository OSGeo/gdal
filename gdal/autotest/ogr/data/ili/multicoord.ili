TRANSFER Multicoord;

  DOMAIN
    LKoord = COORD2 100.00 100.00
                    300.00 300.00;
    LKoord3 = COORD3 100.00 100.00 100.0
                    300.00 300.00 300.0;

MODEL Multicoord

  TOPIC MulticoordTests =

    TABLE MulticoordTable =
      coordPoint1: LKoord;
      coordPoint2: LKoord3;
    NO IDENT
    END MulticoordTable;

  END MulticoordTests.

END Multicoord.

FORMAT FREE;
CODE BLANK = DEFAULT, UNDEFINED = DEFAULT, CONTINUE = DEFAULT;
TID = ANY;

END.
