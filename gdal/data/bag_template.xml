      <gmd:organisationName>
        <gco:CharacterString>${ORGANISATION_NAME:unknown}</gco:CharacterString>
      </gmd:organisationName>
      <gmd:positionName>
        <gco:CharacterString>${POSITION_NAME:unknown}</gco:CharacterString>
      </gmd:positionName>
      <gmd:role>
