
OBJ	=	aigdataset.obj aigopen.obj gridlib.obj aigccitt.obj

GDAL_ROOT	=	..\..

!INCLUDE $(GDAL_ROOT)\nmake.opt

default:	$(OBJ)
	copy *.obj ..\o

all:	$(OBJ) aitest.exe 

aitest.exe:	$(OBJ) aitest.c
	$(CC) $(CFLAGS) aitest.c aigopen.obj gridlib.obj \
			$(GDAL_ROOT)/port/cpl.lib
	
clean:
	-del *.obj

