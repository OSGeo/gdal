CONFIG_VERSION=@GDAL_VERSION@
CONFIG_INST_PREFIX=@GDAL_PREFIX@
CONFIG_INST_LIBS=-L/usr/lib -lgdal
CONFIG_INST_CFLAGS=-I/usr/include
